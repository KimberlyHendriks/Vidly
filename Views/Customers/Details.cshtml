@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li><b>Membership Type</b> @Model.MembershipType.Type</li>
    @if (Model.DateOfBirth.HasValue)
    {
        <li><b>Date of Birth</b> @Model.DateOfBirth.Value.ToShortDateString()</li>
    }
</ul>